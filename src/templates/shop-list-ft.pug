extends partials/layout

//- Override default config variables
append config
  - var vendorStyles = ['nouislider/distribute/nouislider.min.css', 'drift-zoom/dist/drift-basic.min.css']
  - var vendorScripts = ['nouislider/distribute/nouislider.min.js', 'drift-zoom/dist/Drift.min.js']
  - var pageTitle = 'Shop list filters on top'
  - var breadcrumbs = [{'title': 'Shop', 'link': '#'}, {'title': 'List filters on top'}]
  - var parentMenu = 'Shop'

//- Navbar + Quick View Modal
block navbar
  include partials/modal-quick-view
  include partials/navbar-3-level-light

//- Page Title
block page-title
  include partials/page-title-shop-alt
  
//- Page Content
block content

  .container.pb-5.mb-2.mb-md-4

    // Toolbar
    .bg-light.box-shadow-lg.rounded-lg.p-4.mt-n5.mb-4
      .d-flex.justify-content-between.align-items-center
        .dropdown.mr-2
          a(href='#shop-filters' data-toggle='collapse').btn.btn-outline-secondary.dropdown-toggle
            i.czi-filter.mr-2
            | Filters
        .d-flex
          a(href='#').nav-link-style.mr-3
            i.czi-arrow-left
          span.font-size-md 1 / 5
          a(href='#').nav-link-style.ml-3
            i.czi-arrow-right
        .d-none.d-sm-flex
          a(href='shop-grid-ft.html').btn.btn-icon.nav-link-style.mr-2
            i.czi-view-grid
          a(href='#').btn.btn-icon.nav-link-style.bg-primary.text-light.disabled.opacity-100
            i.czi-view-list

      //- shop-filters-collapse.pug template
      include partials/shop-filters-collapse

    .row.justify-content-center

      // Products list
      section.col-lg-8.pt-2
        // Product
        .card.product-card.product-list
          button(type='button' data-toggle='tooltip' data-placement='left' title='Add to wishlist').btn-wishlist.btn-sm
            i.czi-heart
          .d-sm-flex.align-items-center
            a(href='shop-single-v1.html').product-list-thumb
              img(src='img/shop/catalog/01.jpg' alt='Product')
            .card-body.py-2
              a(href='#').product-meta.d-block.font-size-xs.pb-1 Sneakers &amp; Keds
              h3.product-title.font-size-base
                a(href='shop-single-v1.html') Women Colorblock Sneakers
              .d-flex.justify-content-between
                .product-price
                  span.text-accent $154.<small>00</small>
                +star-rating(4)
              .card-body.card-body-hidden
                .pb-2
                  .custom-control.custom-option.custom-control-inline.mb-2
                    input(type='radio', name='size1', id='s-75').custom-control-input
                    label.custom-option-label(for='s-75') 7.5
                  .custom-control.custom-option.custom-control-inline.mb-2
                    input(type='radio', name='size1', id='s-80' checked).custom-control-input
                    label.custom-option-label(for='s-80') 8
                  .custom-control.custom-option.custom-control-inline.mb-2
                    input(type='radio', name='size1', id='s-85').custom-control-input
                    label.custom-option-label(for='s-85') 8.5
                  .custom-control.custom-option.custom-control-inline.mb-2
                    input(type='radio', name='size1', id='s-90').custom-control-input
                    label.custom-option-label(for='s-90') 9
                button(type='button' data-toggle='toast' data-target='#cart-toast').btn.btn-primary.btn-sm.mb-2
                  i.czi-cart.font-size-sm.mr-1
                  | Add to Cart
                .text-left
                  a(href='#quick-view' data-toggle='modal').nav-link-style.font-size-ms
                    i.czi-eye.align-middle.mr-1
                    | Quick view
