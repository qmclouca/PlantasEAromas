  - var pageSubtitle = 'Responsive navigation header that includes support for branding, navigation, and more.'
  - var isPageTitleBadge = true
  - var anchorNav = [{'title': 'Basic example', 'link': '#navbar-basic'}, {'title': 'Color schemes', 'link': '#navbar-color-shemes'}, {'title': 'Topbar', 'link': '#navbar-topbar'}, {'title': 'Topbar + Navbar', 'link': '#navbar-twolevel'}, {'title': 'Multilevel light navbar', 'link': '#navbar-multilevel-light'}, {'title': 'Multilevel dark navbar', 'link': '#navbar-multilevel-dark'}]
  
