            header.navbar.navbar-expand-lg.navbar-light.box-shadow-sm.px-3.rounded
              a(href='#' style='min-width: 7rem;').navbar-brand.d-none.d-lg-block.mr-3.order-lg-1
                +image('img/logo-dark.png', 'Cartzilla')(width='142')
