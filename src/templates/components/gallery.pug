  - var pageTitle = 'Gallery'
  - var pageSubtitle = 'JavaScript based lightbox component for presenting various types of media.'
  - var isPageTitleBadge = true
  - var anchorNav = [{'title': 'Media: Image', 'link': '#gallery-image'}, {'title': 'Media: Video', 'link': '#gallery-video'}, {'title': 'Media: Iframe (Google map)', 'link': '#gallery-iframe'}, {'title': 'Grid with gutters', 'link': '#gallery-grid-gutters'}, {'title': 'Grid no gutters', 'link': '#gallery-grid-nogutters'}, {'title': 'Inside card', 'link': '#gallery-card'}, {'title': 'Inside carousel', 'link': '#gallery-carousel'}]
  
//- Page Content
