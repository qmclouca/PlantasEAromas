  - var pageTitle = 'Shop components'
  - var pageSubtitle = 'Components that are related to the e-commerce module of the website.'
  - var isPageTitleBadge = true
  - var anchorNav = [{'title': 'Product card (Grid)', 'link': '#product-card-grid'}, {'title': 'Product card (List)', 'link': '#product-card-list'}, {'title': 'Product card alt (Downloadable)', 'link': '#product-card-alt'}, {'title': 'Category card', 'link': '#category-card'}, {'title': 'Product gallery', 'link': '#product-gallery'}, {'title': 'Featured category carousel', 'link': '#featured-category-carousel'}, {'title': 'Related products carousel', 'link': '#related-products-carousel'}]
  
